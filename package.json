{
  "name": "paul.laffitte",
  "version": "1.0.0",
  "main": "index.js",
  "license": "MIT",
  "scripts": {
    "start": "yarn run build && (yarn run webpack --watch --config webpack.config.js & bundle exec jekyll serve -d /tmp/jekyll && fg)",
    "build": "yarn run webpack --config webpack.config.js",
    "clean": "bash -c \"rm dist api assets/js node_modules/.bin/{file-loader,terser-webpack-plugin} node_modules/webpack/node_modules/.bin/terser-webpack-plugin -rfv\"",
    "re": "yarn run clean && yarn run build",
    "publish": "./publish.sh"
  },
  "devDependencies": {
    "babel-core": "^6.26.3",
    "babel-loader": "7",
    "babel-preset-es2015": "^6.24.1",
    "extract-loader": "^3.0.0",
    "file-loader": "^2.0.0",
    "webpack": "^4.25.1",
    "webpack-cli": "^3.1.2",
    "yaml-loader": "^0.5.0"
  }
}
